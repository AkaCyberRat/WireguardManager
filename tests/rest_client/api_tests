
###
// Ping
GET http://localhost:5000/ping HTTP/1.1


### 
// Get peer 
GET http://localhost:5000/api/peer HTTP/1.1
Content-Type: application/json

{
    "Id": "1"
}

###
// Create peer
POST http://localhost:5000/api/peer HTTP/1.1
Content-Type: application/json

{
    "PublicKey": "uKIkAl5agqGLoodeDAdtgZHh91vXck5z/mmxETx2dWs=",
    "PresharedKey": "uKIkAl5agqGLoodeDAdtgZHh91vXck5z/mmxETx2dWs=",
    "DownloadSpeed": 11,
    "UploadSpeed": 2,
    "Enabled": false
}

###
// Update peer
PATCH   http://localhost:5000/api/peer HTTP/1.1
Content-Type: application/json

{
    "Id" : "1",
    "Enabled": true
}

###
// Remove peer
DELETE  http://localhost:5000/api/peer HTTP/1.1
Content-Type: application/json

{
    "Id": "1"
}

